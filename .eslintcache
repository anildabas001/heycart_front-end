ideBarHandler={sideBarHandler} />\n      <BackScreen backScreenHandler = {sideBarHandler} showBackScreen={sidebarState.showSideBar}/>\n        <Switch>  \n          <Route path='/' exact  component={Home} />\n          <Route path='/search' exact  component={SearchPage} />\n          <Route path='/:category' exact  component={Category} />\n        </Switch>\n      <Footer />\n    </ NavigationContext.Provider>\n  );\n}\n\nexport default React.memo(App);\n","C:\\Users\\dabas\\Documents\\React Assignments\\heycart_front-end\\src\\Components\\Toolbar\\Toolbar.js",["211","212","213"],"import React, {useContext} from 'react';\r\nimport heyCartLogo from '../../Assets/HeyCartLogo.png';\r\nimport Icon from '../UI/Icon/Icon';\r\nimport classes from './Toolbar.module.css';\r\nimport NavigationItems from '../NavigationItems/NavigationItems';\r\nimport SearchBar from '../NavigationItems/NavigationItem/NavSearchbar/NavSearchbar';\r\nimport NavLink from '../NavigationItems/NavigationItem/NavLink/NavLink';\r\nimport HamBurger from '../HamBurger/HamBurger';\r\nimport NavSearchbar from '../NavigationItems/NavigationItem/NavSearchbar/NavSearchbar';\r\n\r\nconst Toolbar = (props) => {\r\n    return(\r\n        <div className={classes.Toolbar}>             \r\n            <HamBurger crossHamBurger={props.crossHamBurger} clickHandler={props.sideBarHandler} />\r\n            <div className={classes.IconContainer}>\r\n                <Icon source ={heyCartLogo} description='HeyCart logo'/>\r\n            </div>            \r\n            <nav>\r\n                <NavigationItems />                                \r\n            </nav>   \r\n            <span className={classes.smallScreenNav}>\r\n                <NavLink navigationData={\r\n                    {\r\n                        name: 'Cart',\r\n                        linkTo: '/cart',\r\n                        icon: 'IoCartOutline',\r\n                        iconPosition: 'left'\r\n                    }\r\n                } \r\n            />\r\n            </span>               \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default React.memo(Toolbar);","C:\\Users\\dabas\\Documents\\React Assignments\\heycart_front-end\\src\\Components\\UI\\Icon\\Icon.js",[],"C:\\Users\\dabas\\Documents\\React Assignments\\heycart_front-end\\src\\Containers\\Home\\Home.js",[],"C:\\Users\\dabas\\Documents\\React Assignments\\heycart_front-end\\src\\Components\\ImageSection\\ImageSection.js",["214"],"import React from 'react';\r\nimport groceryImage from '../../Assets/grocery-1.jpg'\r\nimport classes from './ImageSection.module.css';\r\n\r\nconst ImageSection = (props) => {\r\n    return (\r\n        <section className={classes.ImageSection}>\r\n            <div className={classes.imageContainer}>\r\n                <img src={groceryImage}/>\r\n                {/* <h1  className={classes.heroText}>Fill your Basket with Happiness!!</h1> */}\r\n            </div>\r\n        </section>\r\n    );\r\n}\r\n\r\nexport default ImageSection;","C:\\Users\\dabas\\Documents\\React Assignments\\heycart_front-end\\src\\Components\\DealSection\\DealSection.js",["215"],"import React from 'react';\r\nimport ProductCard from '../UI/ProductCard/ProductCard';\r\nimport Section from '../UI/Section/Section';\r\nimport SectionHeading from '../UI/SectionHeading/SectionHeading';\r\nimport classes from './DealSection.module.css';\r\nimport ProductSlider from '../UI/ProductSlider/ProductSlider';\r\nimport egg from '../../Assets/egg_cat.jpg'\r\n\r\nconst DealSection = (props) => {    \r\n\r\n    return (\r\n        <Section className={classes.DealSection}>\r\n            <SectionHeading>HeyCart Deals</SectionHeading>\r\n            <ProductSlider>\r\n                {props.data.map(product => <ProductCard key ={product.id} product={{\r\n                    name: product.name,\r\n                    mrp: product.mrp,\r\n                    price: product.price,\r\n                    discount: product.discountPercentage,\r\n        