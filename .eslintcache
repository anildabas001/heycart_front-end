tity > 0 ? <QuantityControl value={cartQuantity} onClickIncrease={onClickIncrease} onClickDecrease={onClickDecrease}/> : \r\n                        props.product.stockQuantity <= 0 ? <p className={classes.stockOut}>Out of Stock</p> : <Button onClickHandler={addToCartHandler}>Add to Cart</Button>\r\n                    }\r\n                </div>                \r\n        </div>\r\n        </Link>\r\n    );\r\n}\r\n\r\nconst mapDispatchToProps = (dispatch => {\r\n    return {\r\n        updateCart: (product, quantity) => dispatch(updateCart(product, quantity)),\r\n        updateCartDb: (productId, quantity) => dispatch(updateCartDb(productId, quantity))\r\n    }\r\n});\r\n\r\nconst mapStateToProps = (state => {\r\n    return { \r\n        cartProducts: state.cartData.products,\r\n        isLoggedin: state.authentication.isLoggedin\r\n    }    \r\n});\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(ProductCard);\r\n","C:\\Users\\dabas\\Documents\\React Assignments\\heycart_front-end\\src\\Components\\UI\\Button\\Button.js",[],"C:\\Users\\dabas\\Documents\\React Assignments\\heycart_front-end\\src\\Components\\UI\\section\\Section.js",[],["301","302"],"C:\\Users\\dabas\\Documents\\React Assignments\\heycart_front-end\\src\\Components\\UI\\SectionHeading\\SectionHeading.js",[],"C:\\Users\\dabas\\Documents\\React Assignments\\heycart_front-end\\src\\Components\\UI\\ProductSlider\\ProductSlider.js",[],"C:\\Users\\dabas\\Documents\\React Assignments\\heycart_front-end\\src\\Components\\CategorySection\\CategorySection.js",["303"],"import React from 'react';\r\nimport Section from '../UI/Section/Section';\r\nimport SectionHeading from '../UI/SectionHeading/SectionHeading';\r\nimport CategoryCard from '../UI/CategoryCard/CategoryCard';\r\nimport fruitsVeg from '../../Assets/fruitsVeg.jpg';\r\nimport beverages from '../../Assets/beverages.jpg';\r\nimport eggs from '../../Assets/egg_cat.jpg';\r\nimport breadDairy from '../../Assets/bread-dairy.jpg';\r\nimp